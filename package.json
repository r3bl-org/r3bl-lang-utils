{
  "name": "r3bl-ts-utils",
  "version": "1.0.1",
  "description": "Typescript language utilities inspired by Kotlin stdlib, and color console",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "preversion": "npm run test",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r3bl-org/r3bl-ts-utils.git"
  },
  "keywords": [
    "color console",
    "let",
    "with",
    "apply",
    "also"
  ],
  "author": "Nazmul Idris",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/r3bl-org/r3bl-ts-utils/issues"
  },
  "homepage": "https://r3bl.com",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.12.2",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "lodash": "^4.17.21",
    "pretty-bytes": "^5.6.0",
    "typescript": "^4.2.2"
  }
}
